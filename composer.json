{
  "name": "andreoneres/brasilapi-php",
  "description": "BrasilAPI PHP Library",
  "type": "lib",
  "homepage": "https://brasilapi.com.br",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BrasilApi\\": "src/"
    }
  },
  "scripts": {
    "dev": "php -S 127.0.0.1:8383",
    "test": "./vendor/bin/pest",
    "coverage": "./vendor/bin/pest --coverage"
  },
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": ">=7.4"
  },
  "require-dev": {
    "pestphp/pest": "^1.22",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
